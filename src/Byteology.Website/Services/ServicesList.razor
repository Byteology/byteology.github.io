<b-services>

    @foreach(var serviceInfo in _servicesInfoRepository.GetAll())
    {
        @if(!serviceInfo.Inactive)
        {
            <ServiceCard Title="@serviceInfo.Title"
                 Description="@serviceInfo.Description"
                 PageLink="@_servicesInfoRepository.GetPaperUrl(serviceInfo.Handle)"
                 Icon="@serviceInfo.Icon" />
        }
    }

</b-services>
